Procedure Find_Norma_all ;
begin
  if wGetTune('TranspPlGsm.NormaDate') = 0
    {
      If Putgsm.cAgregat <> 0
      {
         NRecPos := 0;
         Find_Norma_Agt;
      }
      else
      {
        if Putgsm.EdinRas = 0 or  Putgsm.EdinRas = 4 or Putgsm.EdinRas = 5
// без учета марки прицепа...
           NRecPos := 0;
        else
           NRecPos := PutGsm.CardRec;  // МАРКА ПРИЦЕПА
        if Putgsm.EdinRas = 6
           NRecPos_usl := PutGsm.cRashDoc;  //УСЛУГА
        else
           NRecPos_usl := 0;
        if Putgsm.EdinRas = 3 or  Putgsm.EdinRas = 5
           NRecPos_Uch := Putgsm.cMaravt;
        else
           NRecPos_Uch := 0;
        if Putgsm.EdinRas = 2 or  Putgsm.EdinRas = 4
           Find_Norma_Mar
        else
        if Putgsm.EdinRas = 3 or  Putgsm.EdinRas = 5
           Find_Norma_Mar;
        else
        if Putgsm.EdinRas = 6
           Find_Norma_Mar;
        else
        if Putgsm.EdinRas = 7
           Find_Norma_Mar_NoTC;
        else
           Find_Norma;
       }
    }
    else
    {
      If Putgsm.cAgregat <> 0
      {
         NRecPos := 0;
         Find_Norma_Agt_Date;
      }
      else
      {
        if Putgsm.EdinRas = 0 or  Putgsm.EdinRas = 4 or Putgsm.EdinRas = 5
// без учета марки прицепа...
           NRecPos := 0;
        else
           NRecPos := PutGsm.CardRec;  // МАРКА ПРИЦЕПА
        if Putgsm.EdinRas = 6
           NRecPos_usl := PutGsm.cRashDoc;  //УСЛУГА
        else
           NRecPos_usl := 0;
        if Putgsm.EdinRas = 3 or  Putgsm.EdinRas = 5
           NRecPos_Uch := Putgsm.cMaravt;
        else
           NRecPos_Uch := 0;
        if Putgsm.EdinRas = 2 or  Putgsm.EdinRas = 4
           Find_Norma_Mar_Date
        else
        if Putgsm.EdinRas = 3 or  Putgsm.EdinRas = 5
           Find_Norma_Mar_Date;
        else
        if Putgsm.EdinRas = 6
           Find_Norma_Mar_Date;
        else
        if Putgsm.EdinRas = 7
           Find_Norma_Mar_NoTC_Date;
        else
           Find_Norma_Date;
       }
    }

end;