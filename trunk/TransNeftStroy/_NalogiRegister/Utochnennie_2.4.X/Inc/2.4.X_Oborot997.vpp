procedure GetSaldo997_34;
{
  Saldo997_34 := 0;
  if (isvalidall(#SALDMOUN997_34))
  {
    logStrToFile('C:\Col11', 'Расчитывается сальдо по 997.34(Дебет -, Кредит +)');
   _loop SALDMOUN997_34
     if SALDMOUN997_34.VIOB = word(2)
     {
       Saldo997_34 := Saldo997_34 + SALDMOUN997_34.sums;
       logStrToFile('C:\Col11','Это складывается '+KatOs.NameOs +' '+SALDMOUN997_34.DBSCHETO+'.'+SALDMOUN997_34.SUBOSSCH+' '+ ' '+SALDMOUN997_34.sums );
     }
     else
       if SALDMOUN997_34.VIOB = word(1)
       {
         Saldo997_34 := Saldo997_34 - SALDMOUN997_34.sums;
         logStrToFile('C:\Col11','Это отнимается '+KatOs.NameOs +' '+SALDMOUN997_34.DBSCHETO+'.'+SALDMOUN997_34.SUBOSSCH+' '+ ' '+SALDMOUN997_34.sums );
       }

  }
}

