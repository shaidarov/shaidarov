     Sc1 := Oborot997_34_Utoch.SUBOSSCH;
     if Sc='920'
       or Sc='923'
       or Sc='925'
       or Sc='926'
       or Sc='991'
       or Sc='944'
       or (Sc='929' and Sc1 = '01')
       or (Sc='929' and Sc1 = '02')
