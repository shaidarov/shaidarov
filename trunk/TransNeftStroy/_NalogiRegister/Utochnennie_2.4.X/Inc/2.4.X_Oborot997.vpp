procedure GetOborot997_33;
{
  var OborotK : double;

  Oborot997_33:= 0;
  OborotK := 0;
  logStrToFile('C:\Col11', 'Рассчитываются проводки по К997.33');
  _Loop Oborot997_33K
  {
    OborotK := OborotK + Oborot997_33K.SUMOB;
    LogStrToFile('C:\Col11.log',KatOS.NameOS+' Д'+Oborot997_33K.SCHETO+'.'+Oborot997_33K.SUBOSSCH+' К'+Oborot997_33K.SCHETK+'.'+Oborot997_33K.SUBSCHK +' от '+Oborot997_33K.DatOb+' '+Oborot997_33K.SUMOB);
  }

  if (isvalidall(tnOborot))
  {
    logStrToFile('C:\Col11', 'Рассчитываются проводки по L997.33');
    _Loop Oborot
    {
      Oborot997_33 := Oborot997_33 + oborot.SUMOB;
      LogStrToFile('C:\Col11.log',KatOS.NameOS+' Д'+Oborot.SCHETO+'.'+Oborot.SUBOSSCH+' К'+Oborot.SCHETK+'.'+Oborot.SUBSCHK +' от '+Oborot.DatOb+' '+Oborot.SUMOB);
    }
  }
  logStrToFile('C:\Col11', KatOS.NameOS+' Д997.33 - K997.33 = ' + Oborot997_33+'-'+OborotK);
  Oborot997_33 := Oborot997_33 - OborotK;
  logStrToFile('C:\Col11', '='+Oborot997_33);
}

