[database]
DataBaseName=GAL
DataBaseDriver=ORA90DRV.DLL
CheckRepository=off
FiltersResource="\\fs01-tns\apps\gal\Filters\FILTERS.RES"

[SQLDriver]
SQLServer=DB01TNS-outer
FullLoginName=ON

! Тип визуализации по умолчанию (NUMERIC, INDICATOR или ROTATE)
System.VisualType=NUMERIC
! Сжимать ресурсы (быстрое сжатие отключаем)
System.ResourceFastCompress=Off
! Путь на каталог, где будут храниться отчеты и другие выходные данные 
Files.OutputFilesDirectory=IncVipLo 
! Путь на каталог где ведутся временные файлы (например, C:\TMP)
Files.TmpFilesDirectory="C:\Temp\"
!"%user%\tmp\"
!"\\fs01-tns.tns.transneft.ru\apps\gal\dsk\MihaylovYV\tmp\" 
!Macro.IncludeEatLine=True
[Compilers] 
! Уровень выдачи диагностики (0-только Error, 1-Warning и Error, 2-все : Warning, Info, Error)
  ErrorLevel=2 
! Режим минимизации выдачи информации о процессе компиляции с целью ее ускорения 
 MinVisual=On  
! Формировать Include-файлы для паскаля в результате компиляции
  WriteIncludes=Off 
! Служебный ресурс 
  SubServientResource='atlantis.res' 
! При задании файлов по маске, искать также файлы в поддиректориях 
  SubDir=OFF 
! Результат компиляции сохранять в ресурсе
  WriteToResource=On 
! Формировать листинги в результате компиляции
  WriteListing=ON 
! Короткий формат вывода ошибок  
 ShortMessages=OFF 
! Назначение : Компилировать все заданные файлы, а не до первой ошибки 
 Full=ON 
! Для выявления нелицензированных интерфейсов при компиляции  
 DisableIfcLicWarnings = false 
! Предпочтение константам при компиляции  
 ConstantPreference=off 

! вместо компиляции выдает на консоль список измененных файлов 
CheckModifyOnly=OFF 
! Сохранять информацию о наследовании интерфейсов 
InhSaveLevel=3 
[Vip] 
! Маска предупреждения при приведении типов: 0-нет; 1-к целочисленным типам; 
! 2-к вещественным типам; 4-к строковым типам; 8-к типам дата-время; 
BaseTypesWarning=14                                        
! В 8.1 используется некомпонентный словарь 
NoTablesInComponent=On                      
! Запрещено включать параметры Debug и LocalDebug при сборке версии  
Debug=0 
LocalDebug=0 


