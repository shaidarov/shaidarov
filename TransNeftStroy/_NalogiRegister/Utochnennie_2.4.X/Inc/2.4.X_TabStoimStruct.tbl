table Struct TabStoimStruct(
  cTabKatOS     : comp,
  Schet         : string,
  cVid          : comp,
  Vid           : string,
  cPodrazd      : comp,
  Podrazd       : string,
  Percent       : double,
  SumOplVh      : double,
  SumNachVh     : double,
  UtochSum      : double
)with index(
  TabStoimStruct0=cTabKatOS,
  TabStoimStruct1=cTabKatOS+Schet+cVid+cPodrazd,
  TabStoimStruct3=Schet+cVid+cTabKatOS
);
