table Struct TabStoimStruct(
  cTabKatOS     : comp,
  Schet         : string,
  cVid          : comp,
  Vid           : string,
  cPodrazd      : comp,
  Podrazd       : string,
  Percent       : double,
  SumOplVh      : double,
  SumNachVh     : double
)with index(
  TabStoimStruct0=cTabKatOS,
  TabStoimStruct1=Schet+cVid+cPodrazd,
  TabStoimStruct2=Schet+cVid
);
