             else
               {
                LogStrToFile(GetStringParameter('Files','TmpFilesDirectory',0)+'saldo_76.log','Виды расчетов по претензиям|код|'+trim(DBFGetFieldValue(Handle,'KODVRPP')));
               }
           }

!-------------end  Виды расчетов по претензиям ---------------------

!-------------  Виды страхования ---------------------

          if vs_ID <> 0
