              vpru_ID := 0;
             }
           }

          if (vpru_ID = 0 and trim(DBFGetFieldValue(Handle,'KODVPRU')) <> '')
           {
            if getfirst spkau where (( trim(DBFGetFieldValue(Handle,'KODVPRU')) == spkau.code  and word(20009) == spkau.KODGRKAU )) = tsok
             {
              vpru_ID := spkau.nrec;
             }
             else
               {
                LogStrToFile(GetStringParameter('Files','TmpFilesDirectory',0)+'saldo_76.log','Виды продукции, работ, услуг|код|'+trim(DBFGetFieldValue(Handle,'KODVPRU')));
               }
           }

!------------- end Виды продукции, работ, услуг ---------------------

!-------------  Виды расчетов по претензиям ---------------------

          if vrp_ID <> 0
           {
           if getfirst spkau where ((vrp_ID == spkau.nrec )) = tsok
            {
             vrp_ID := spkau.nrec;
            }
            else
             {
              LogStrToFile(GetStringParameter('Files','TmpFilesDirectory',0)+'saldo_76.log','Виды расчетов по претензиям|нрек|'+vrp_ID);
              vrp_ID := 0;
             }
           }

          if (vrp_ID = 0 and trim(DBFGetFieldValue(Handle,'KODVRPP')) <> '')
