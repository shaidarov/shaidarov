            if getfirst spkau where (( trim(DBFGetFieldValue(Handle,'KODVS')) == spkau.code  and word(20025) == spkau.KODGRKAU )) = tsok
             {
              vs_ID := spkau.nrec;
             }
             else
               {
                LogStrToFile(GetStringParameter('Files','TmpFilesDirectory',0)+'saldo_76.log','Виды страхования|код|'+trim(DBFGetFieldValue(Handle,'KODVS')));
