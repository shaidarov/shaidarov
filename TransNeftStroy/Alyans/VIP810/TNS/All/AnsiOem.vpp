 w:=''
 for(i:=1;i<=length(wstr);i:=i+1)
 begin
//   k:=pos(substr(wstr,i,1),ansistr)
   pch:=c_tblAns[ord(substr(wstr,i,1))] ;
   if pch>chr(0) then
//     wdm[i]:=pch
     w:=w+pch
   else 
