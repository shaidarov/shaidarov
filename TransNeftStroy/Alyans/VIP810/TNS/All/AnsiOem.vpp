Function OemAnSi(wstr:string) : string ;
var w : string ;
    k,i :longint ;
begin
 w:='' ;
 for(i:=1;i<=length(wstr);i:=i+1)
 begin
   k:=pos(substr(wstr,i,1),oemstr)
   if k>0 then
     w:=w+substr(ansistr,k,1) 
   else 
     w:=w+substr(wstr,i,1) ;
 end ;
 OemAnsi:=w ;
end ;